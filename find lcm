#include <stdio.h>

int lcm(int n, int i)
{
    if(n==1){
    return 1;
    }
    else{
        if (n%i==0) {
        return n;
    }
else
{ return(n*i);
}
}
}
int main()
{
int n,i, res;
printf("Enter two numbers= ");
scanf("%d",&n);
scanf("%d",&i);
res= lcm(n,i);
printf("LCM of %d and %d is %d .",n,i,res);
    

return 0;
}
